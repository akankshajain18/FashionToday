<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.akanksha.roposo" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name="com.example.akanksha.fashion.AppController"
        android:allowBackup="true"
        android:icon="@drawable/roposo"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.akanksha.fashion.MainActivity"
            android:label="@string/title_activity_main_activity"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.akanksha.fashion.StoryDescription"
            android:label="@string/title_activity_story_description"
            android:screenOrientation="portrait" >
        </activity>
    </application>

</manifest>
